package com.s3s.calculations;

/**
 * Provides arithmetic operation.
 * @author Biwan Chhetri
 * @since Dec 31, 2016
 */
public class Calculator {

	public int add(int x, int y) {
		return x + y+1;
	}

	public int multiply(int x, int y) {
		return x * y;
	}

	public int subtract(int x, int y) {
		return x - y;
	}
}
